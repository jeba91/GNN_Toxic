{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "GIN",
    "dataset": "TOX",
    "out_dir": "out/TOX_graph_classification/",
    "params": {
        "seed": 79,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-10,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 48
    },
    "net_params": {
        "L": 6,
        "hidden_dim": 192,
        "residual": true,
        "readout": "sum",
        "n_mlp_GIN": 2,
        "learn_eps_GIN": true,
        "neighbor_aggr_GIN": "sum",
        "in_feat_dropout": 0.5,
        "dropout": 0.5,
        "graph_norm": true,
        "batch_norm": true
    }
}