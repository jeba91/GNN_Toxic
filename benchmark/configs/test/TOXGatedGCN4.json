{
    "gpu": {
        "use": true,
        "id": 0
    },
    "model": "GatedGCN",
    "dataset": "TOX",
    "out_dir": "out/TOX_graph_classification/",
    "params": {
        "seed": 79,
        "epochs": 1000,
        "batch_size": 5,
        "init_lr": 0.0001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-10,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 48
    },
    "net_params": {
        "L": 6,
        "hidden_dim": 64,
        "out_dim": 64,
        "residual": true,
        "readout": "sum",
        "in_feat_dropout": 0.5,
        "dropout": 0.5,
        "graph_norm": true,
        "batch_norm": true,
        "edge_feat": true
    }
}